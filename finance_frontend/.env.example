# Frontend environment variables
# Note: Do not commit .env with secrets; this file documents required keys.

# Backend base URL for API calls (preferred)
REACT_APP_API_URL=http://localhost:3001

# Alternative variable the app will also read if REACT_APP_API_URL is not set
REACT_APP_BACKEND_URL=http://localhost:3001

# Optional additional environment variables used by the hosting environment
REACT_APP_API_BASE=
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=
